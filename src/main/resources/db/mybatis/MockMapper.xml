<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.emmm.MockMapper">
    <insert id="insertNews" parameterType="com.github.emmm.News">
        insert into NEWS (url,title,content,created_at,modified_at)
        values(#{url}, #{title}, #{content}, #{createdAt}, #{modifiedAt})
    </insert>
    <select id="selectNews" resultType="com.github.emmm.News">
        select url,title,content,created_at,modified_at
        from NEWS
    </select>

</mapper>