create table LINKS_TO_BE_PROCESSED
(
  link varchar(100)
);

create table LINKS_ALREADY_PROCESSED
(
  link varchar(100)
);

create table NEWS(
  id bigint primary key auto_increment,
  title text,
  content text,
  url varchar(100),
  created_at timestamp,
  modified_at timestamp
);